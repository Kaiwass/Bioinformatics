GCGGCGGAGT"""

new = string.split()
name = []
strand = []
for i in range(len(new)):
    st = ''
    if '>' in new[i]:
        name.append(new[i][1:])
        for y in range(i+1, len(new)):
            if '>' in new[y]:
                strand.append(st)
                break
            st += new[y]
            if y == len(new)-1:
                strand.append(st)

motif = strand[1]
x = 0
indices = []
for letter in motif:
    for y in range(x, len(strand[0])):
        if letter == strand[0][y]:
            indices.append(str(y + 1))
            x = y + 1
            break
print(" ".join(indices))
